<h6> Rate how strongly you feel your mood (5 = very strongly. 1 = not so much.)</h6> 

<%= form_with model: @mood_log do |f| %>
    <%= f.label :user %>
    <% if !@mood_log.id %>
        <%=  f.text_field :user_name, list: "users_autocomplete" %>
        <datalist id="users_autocomplete">
            <% @users.each do |user| %>
                <option value="<%= user.name %>">
                <% end %>
        </datalist>
    <% else %>
        <%= @mood_log.user_name %>
    <% end %>
<br>
    <%= f.label :mood %>
    <%=  f.text_field :mood_name, list: "moods_autocomplete"%>
    <datalist id="moods_autocomplete">
        <% @moods.each do |mood| %>
            <option value="<%= mood.name %>">
            <% end %>
    </datalist>
<br>
    <%= f.label :rating %>
    <%= f.collection_select :rating_id, Rating.all, :id, :level %> 
<br>
    <%= f.submit %>
<% end %>